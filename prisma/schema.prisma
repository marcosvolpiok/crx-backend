// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url          = "postgresql://postgres:legaltech@localhost:5432/mydb?schema=legaltech"
}

model Search {
  id    Int     @default(autoincrement()) @id
  priceMin Int
  priceMax Int
  rooms  String
  type String
  label String
}

model Result {
  id    Int     @default(autoincrement()) @id
  bot_id Int
  url String
  title String
  price Int
  price_history String
  status String
  address_line String
  neighborhood_name String
  totalArea Int
  coveredArea Int
  uncoveredArea Int
  category String
}